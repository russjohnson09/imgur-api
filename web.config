<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <defaultDocument>
            <files>
                <add value="index.php" />
            </files>
        </defaultDocument>
        <rewrite>
            <rules>
                <rule name="plugin">
                    <match url="plugin-demo" />
                    <action type="Rewrite" url="plugin-demo.php" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>
